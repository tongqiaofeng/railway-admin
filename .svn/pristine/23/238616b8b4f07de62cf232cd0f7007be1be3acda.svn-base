<div style=\"display: flex;\">\n            <div class=\"right-div\" @click=\"updateUserClick\">\n              <span class=\"span1\">{{ userNick }}</span>\n              <div class=\"span-img\">\n                <img\n                  src=\"../assets/imgs/home/down.png\"\n                  style=\"width: 15px;margin-left: 10px;\"\n                  class=\"span-img\"\n                />\n              </div>\n            </div>\n            <div @click=\"warehouseWarn\" class=\"loginoutDiv\">\n              <img\n                class=\"loginout\"\n                title=\"实时报警\"\n                src=\"../assets/imgs/home/warn.png\"\n              />\n              <div class=\"warnNum\">\n                <span>{{ total > 99 ? \"99+\" : total }}</span>\n              </div>\n            </div>\n            <el-drawer\n              title=\"实时报警\"\n              :visible.sync=\"drawer\"\n              direction=\"rtl\"\n              size=\"860px\"\n            >\n              <el-table :data=\"warnListPage\" style=\"width: 100%\">\n                <el-table-column label=\"报警仓库\">\n                  <template slot-scope=\"scope\">\n                    <div>\n                      <p\n                        :style=\"{\n                          color: scope.row.flag == 0 ? 'red' : '#3d81fd',\n                          margin: 0,\n                        }\"\n                      >\n                        {{ scope.row.warehouseName }}\n                      </p>\n                    </div>\n                  </template>\n                </el-table-column>\n                <el-table-column label=\"报警内容\" width=\"300px\">\n                  <template slot-scope=\"scope\">\n                    <div>\n                      <p\n                        :style=\"{\n                          color: scope.row.flag == 0 ? 'red' : '#3d81fd',\n                          margin: 0,\n                        }\"\n                      >\n                        {{ scope.row.note }}\n                      </p>\n                    </div>\n                  </template>\n                </el-table-column>\n                <el-table-column label=\"报警时间\" width=\"160px\">\n                  <template slot-scope=\"scope\">\n                    <div>\n                      <p\n                        :style=\"{\n                          color: scope.row.flag == 0 ? 'red' : '#3d81fd',\n                          margin: 0,\n                        }\"\n                      >\n                        {{ scope.row.time }}\n                      </p>\n                    </div>\n                  </template>\n                </el-table-column>\n                <el-table-column label=\"操作人\">\n                  <template slot-scope=\"scope\">\n                    <div>\n                      <span\n                        :style=\"{\n                          color: scope.row.flag == 0 ? 'red' : '#3d81fd',\n                        }\"\n                        >{{\n                          scope.row.name == \"\" ? \"暂未处理\" : scope.row.name\n                        }}</span\n                      >\n                    </div>\n                  </template>\n                </el-table-column>\n                <el-table-column label=\"状态\">\n                  <template slot-scope=\"scope\">\n                    <div>\n                      <span\n                        :style=\"{\n                          color: scope.row.flag == 0 ? 'red' : '#3d81fd',\n                        }\"\n                        >{{\n                          scope.row.flag == 0\n                            ? \"报警\"\n                            : scope.row.flag == 1\n                            ? \"处理中\"\n                            : \"处理完成\"\n                        }}</span\n                      >\n                    </div>\n                  </template>\n                </el-table-column>\n                <el-table-column label=\"操作\">\n                  <template slot-