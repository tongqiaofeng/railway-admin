num);\r\n    if (num !== null && num !== undefined) {\r\n      let parts = num.toString().split(\".\");\r\n      parts[0] = parts[0].replace(/\\B(?=(\\d{3})+(?!\\d))/g, \",\");\r\n      return parts.join(\".\");\r\n    } else {\r\n      return 0;\r\n    }\r\n  };\r\n  Vue.prototype.formatStringRgx = function (param) {\r\n    // console.log(param);\r\n    let str = param.substring(0, param.length - 9);\r\n    // console.log(str);\r\n    return str;\r\n  };\r\n};"]}]}