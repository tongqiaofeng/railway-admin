 </el-cascader>\r\n        </el-form-item>\r\n        <el-form-item label=\"仓库：\">\r\n          <el-select v-model=\"warehouseIdList\" multiple placeholder=\"可多选\" @change=\"wareCheck\">\r\n            <el-option v-for=\"ware in warehouseSelList\" :key=\"ware.id\" :label=\"ware.name\" :value=\"ware.id\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"类型：\">\r\n          <el-select v-model=\"flag\" placeholder=\"请选择\">\r\n            <el-option v-for=\"ware in flagList\" :key=\"ware.id\" :label=\"ware.name\" :value=\"ware.id\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"inventoryCheck\">查 询</el-button>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\">\r\n            <a :href=\"eurl\" class=\"urlimg\" @click=\"exportExcel\">导出excel表格</a>\r\n          </el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n    <div>\r\n      <el-table :data=\"stockStatisticsList\" tooltip-effect=\"dark\" border>\r\n        <el-table-column prop=\"supplierName\" label=\"厂家\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"systemName\" label=\"系统\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"warehouseName\" label=\"仓库\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"deviceName\" label=\"设备名称\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"deviceModel\" label=\"设备型号\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"benchmark\" label=\"生产批次\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"productTime\" label=\"生产日期\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"stockInTime\" label=\"入库日期\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"stockInDays\" label=\"库存时长（天）\">\r\n        </el-table-column>\r\n      </el-table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        stockStatisticsList: [],\r\n        wid: '',\r\n        warehouseIdList: [],\r\n        flag: 0,\r\n        propsMenu: {\r\n          checkStrictly: true,\r\n          emitPath: false,\r\n          value: 'id',\r\n          label: 'label'\r\n        },\r\n        menuList: [],\r\n        warehouseList: [],\r\n        warehouseSelList: [{\r\n          id: 0,\r\n          name: '全部'\r\n        }],\r\n        flagList: [{\r\n          id: 0,\r\n          name: '库存中'\r\n        }, {\r\n          id: 1,\r\n          name: '维修中'\r\n        }, {\r\n          id: 2,\r\n          name: '检测中'\r\n        }, ],\r\n        eurl: '',\r\n\r\n      }\r\n    },\r\n    created() {\r\n      this.getWarehouseList();\r\n      this.inventoryCheck();\r\n    },\r\n    methods: {\r\n      // 导出Excel表格\r\n      exportExcel() {\r\n        this.$axios.post(this.$store.state.baseUrl + '/stockExportData', {\r\n          wid: this.wid,\r\n          warehouseIdList: this.warehouseIdList.indexOf(0) !== -1 ? [] : this.warehouseIdList,\r\n          flag: this.flag\r\n        }).then((res) => {\r\n          console.log(res);\r\n          console.log(this.$store.state.baseUrl + res.data.excelUrl);\r\n          this.eurl = this.$store.state.baseUrl + res.data.excelUrl;\r\n        }).catch((err) => {\r\n          console.log(err);\r\n        })\r\n      },\r\n      // 库存统计\r\n      inventoryCheck() {\r\n        this.$axios.post(this.$store.state.baseUrl + '/stockStatistics', {\r\n          wid: this.wid,\r\n          warehouseIdList: this.warehouseIdList.indexOf(0) !== -1 ? [] : this.warehouseIdList,\r\n          flag: this.flag\r\n        }).then(res => {\r\n          console.log('库存统计');\r\n          console.log(res);\r\n          this.stockStatisticsList = res.data;\r\n        }).catch(err => {\r\n          console.log(err);\r\n        })\r\n      },\r\n      // 仓库选择\r\n      wareCheck() {\r\n        console.log(this.warehouseIdList);\r\n        // if (this.warehouseIdList.indexOf(0) !== -1) {\r\n        //   for (const ite of this.warehouseSelList) {\r\n        //     if (ite.id !== 0) {\r\n        //       this.warehouseIdList.push(ite.id);\r\n        //     }\r\n        //   }\r\n        // }\r\n      },\r\n      // 部门下的仓库\r\n      menuChange(value) {\r\n        console.log(value);\r\n        this.warehouseSelList = [{\r\n          id: 0,\r\n          name: '全部'\r\n        }];\r\n        for (const item of this.warehouseList) {\r\n          if (item.wid == value) {\r\n            this.warehouseSelList.push(item);\r\n          }\r\n        }\r\n      },\r\n      // 获取部门、仓库列表\r\n      getWarehouseList() {\r\n        this.$axios.post(this.$store.state.baseUrl + '/menuWarehouseList').then((res) => {\r\n          console.log('部门列表');\r\n          console.log(res);\r\n          this.menuList = res.data.menuList;\r\n          this.warehouseList = res.data.warehouseList;\r\n        }).catch((err) => {\r\n          console.log(err);\r\n        });\r\n      },\r\n    }\r\n  }\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n  .inventory-statistics-container {\r\n    // width: 100%;\r\n    padding: 20px;\r\n    background-color: #fff;\r\n    border-radius: 6px;\r\n\r\n    .urlimg {\r\n      text-decoration: none;\r\n      color: #fff;\r\n    }\r\n  }\r\n</style>"]}]}