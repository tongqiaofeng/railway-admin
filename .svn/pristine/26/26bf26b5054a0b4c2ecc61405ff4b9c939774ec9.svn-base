     <span\n                        :style=\"{color: scope.row.flag == 0 ? 'red' : '#3d81fd'}\">{{scope.row.flag == 0 ? '报警' : (scope.row.flag == 1 ? '处理中' : '处理完成')}}</span>\n                    </div>\n                  </template>\n                </el-table-column>\n                <el-table-column label=\"操作\">\n                  <template slot-scope=\"scope\">\n                    <div>\n                      <el-button v-if=\"scope.row.flag == 0\" type=\"text\" @click=\"warnDispose(scope.row)\"\n                        style=\"color: red;\">确认</el-button>\n                      <el-dialog title=\"处理报警\" :visible.sync=\"dialogWarnVisible\" :close-on-press-escape=\"false\"\n                        :close-on-click-modal=\"false\" :append-to-body=\"true\">\n                        <div style=\"text-align: center;\">\n                          <p style=\"font-size: 16px;\">确定处理该报警？</p>\n                        </div>\n                        <div slot=\"footer\">\n                          <el-button @click=\"dialogWarnVisible = false\">取 消</el-button>\n                          <el-button type=\"primary\" @click=\"warnDisposeSure\">确 定</el-button>\n                        </div>\n                      </el-dialog>\n                    </div>\n                  </template>\n                </el-table-column>\n              </el-table>\n              <div style=\"width: 100%;height: 50px;\">\n                <div style=\"margin:15px 0;position:absolute;right:6%;\">\n                  <el-pagination @current-change=\"handleCurrentChange\" :current-page=\"page\"\n                    layout=\"total, prev, pager, next, jumper\" :total=\"total\">\n                  </el-pagination>\n                </div>\n              </div>\n            </el-drawer>\n          </div>\n          <div class=\"downbtn\" v-if=\"updateUser==1\">\n            <ul class=\"updateUl\">\n              <li class=\"updateLi1\">\n                <div class=\"updateP\" @click=\"pswUpdate\">\n                  <div class=\"nick-img\">\n                    <div class=\"update-nick-img\" style=\"display: flex;\">\n                      <div style=\"margin-top: 3px;\">\n                        <img src=\"../assets/imgs/update-psw.png\" style=\"width: 17px;height: 20px;\" />\n                      </div>\n                      <span>修改密码</span>\n                    </div>\n                  </div>\n                </div>\n                <el-dialog title=\"修改密码\" :visible.sync=\"pswDialogVisible\" :append-to-body=\"true\"\n                  :close-on-press-escape=\"false\" :close-on-click-modal=\"false\">\n                  <div style=\"width:100%;display:flex;\">\n                    <p class=\"font\">旧密码：</p>\n                    <input class=\"update-nick\" type=\"text\" v-model=\"oldPsw\" placeholder=\"请输入旧密码\" />\n                  </div>\n                  <div style=\"width:100%;margin-top:15px;display:flex;\">\n                    <p class=\"font\">新密码：</p>\n                    <input class=\"update-nick\" type=\"text\" v-model=\"newPsw\" placeholder=\"请输入新密码\" />\n                  </div>\n                  <span slot=\"footer\" class=\"dialog-footer\">\n                    <el-button @click=\"pswDialogVisible = false\" style=\"margin-left: 35%;\">取 消</el-button>\n                    <el-button type=\"primary\" @click=\"updatePsw\" v-preventClick>确 定</el-button>\n                  </span>\n                </el-dialog>\n              </li>\n              <li class=\"updateLi1\">\n                <div class=\"updateP\" @click=\"logOut\">\n                  <div class=\"psw-img\">\n                    <div class=\"update-nick-img\" style=\"display: flex;\">\n                      <div style=\"margin-top: 3px;\">\n                        <img src=\"../assets/imgs/logOut.png\" style=\"width: 17px;height: 18px;\" />\n                      </div>\n                      <span>退出登录</span>\n                    </div>\n                  </div>\n                </di