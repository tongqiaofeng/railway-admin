en) {\n      if (token) {\n        state.token = token;\n      } else {\n        state.token = null;\n      }\n    },\n\n    nick(state, nick) {\n      if (nick) {\n        state.nick = nick;\n      } else {\n        state.nick = '';\n      }\n    },\n\n    allowBack(state, allowBack) {\n      state.allowBack = false;\n      allowBack\n    },\n\n  },\n  actions: {\n    // 应用mutations\n    setToken({\n      commit\n    }, token) {\n      commit('userToken', token)\n    },\n\n    setNick({\n      commit\n    }, nick) {\n      commit('nick', nick)\n    },\n\n    updateAppSetting({\n      commit\n    }, allowBack) {\n      allowBack\n      commit('allowBack', false)\n    },\n  },\n  modules: {}\n});"]}]}